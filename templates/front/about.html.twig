{%extends "base.html.twig" %} 
{% block title %}About{% endblock %}

{% block body %}

<div>
    {{ include("front/template_part/_menu.html.twig", {pp: 'about'} )}}
</div>
<div class="container mt-3">
    <h2>About Us</h2>
    <div class="col-sm-9"> 
        <p>Here you can discover and learn all about our travelling agency !</p>
    </div>
    <div class=""> 
    <h3>Find out more about our Nature Voyages<h5>
        {% for article in articles %}
        <pre>
            {{ dump(article) }}
        </pre>
        <div class="card">
            <h4 class="card-header">{{ article.label}}</h4>
            <div class="card-body">
                <h6 class="card-title">{{ article.author}}</h6>
                <p class="">{{ article.description}}</p>
                {# <p class="card-text">{{ article.getPublishDate}}</p>  #}
            </div>
        </div>
        {% endfor %}
    </div>
</div>
    
{% endblock %}